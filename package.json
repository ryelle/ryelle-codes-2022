{
  "name": "ryelle-2022",
  "scripts": {
    "build": "postcss src/*.css --dir ./build/ --no-map",
    "start": "postcss src/*.css --dir ./build/ --watch",
    "lint:css": "stylelint src"
  },
  "dependencies": {
    "@wordpress/stylelint-config": "20.0.2",
    "autoprefixer": "10.4.8",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "postcss-import": "14.1.0",
    "postcss-nesting": "11.3.0",
    "postcss-short": "5.0.0",
    "stylelint": "14.9.1"
  },
  "browserslist": "> 1% and not ie <= 11",
  "stylelint": {
    "extends": "@wordpress/stylelint-config",
    "rules": {
      "no-descending-specificity": null,
      "selector-class-pattern": null
    }
  }
}
