{
  "name": "ryelle-2022",
  "scripts": {
    "build": "postcss src/*.css --dir ./build/ --no-map",
    "start": "postcss src/*.css --dir ./build/ --watch",
    "lint:css": "stylelint src"
  },
  "dependencies": {
    "@wordpress/stylelint-config": "23.22.0",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "postcss-import": "16.1.1",
    "postcss-nesting": "13.0.2",
    "postcss-short": "5.0.0",
    "stylelint": "16.21.1"
  },
  "browserslist": "> 1% and not ie <= 11",
  "stylelint": {
    "extends": "@wordpress/stylelint-config",
    "rules": {
      "no-descending-specificity": null,
      "selector-class-pattern": null
    }
  }
}
