	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--navy)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/code": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--sky)",
					"text": "var(--wp--preset--color--navy)"
				},
				"spacing": {
					"padding": {
						"bottom": "1rem",
						"left": "1rem",
						"right": "1rem",
						"top": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--code)",
					"fontSize": "var(--wp--custom--font-size--code)"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/comment-date": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "calc(var(--wp--custom--spacing--vertical) / 4)",
						"top": "0"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--header)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--header)"
				}
			},
			"core/navigation": {
				"typography": {
					"lineHeight": "1"
				}
			},
			"core/post-content": {
				"spacing": {
					"blockGap": "2rem"
				}
			},
			"core/post-date": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--main)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontWeight": "500"
				}
			},
			"core/query-pagination": {
				"spacing": {
					"margin": {
						"top": "0"
					},
					"padding": {
						"bottom": "var(--wp--custom--spacing--horizontal)",
						"top": "var(--wp--custom--spacing--horizontal)"
					}
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--blue)",
					"style": "solid",
					"width": "0 2px"
				},
				"color": {
					"background": "var(--wp--preset--color--sky)",
					"text": "var(--wp--preset--color--navy)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--custom--spacing--horizontal)",
						"left": "var(--wp--custom--spacing--horizontal)",
						"right": "var(--wp--custom--spacing--horizontal)",
						"top": "var(--wp--custom--spacing--horizontal)"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--header)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--navy)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--header)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--line-height--header)"
				}
			},
			"link": {
